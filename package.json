{
  "name": "arent-web",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "generate:rm": "find . -regex './src/.*__generated__.*' -type d -prune -exec rm -r \"{}\" \\;",
    "start": "next start",
    "lint": "eslint --fix .",
    "commit": "git-cz",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,md,json}\" --config ./.prettierrc",
    "codegen:generate": "graphql-codegen --config codegen.yml",
    "prepare": "husky install && chmod ug+x .husky/*",
    "schema:download": "apollo-codegen introspect-schema http://localhost:3001/graphql.io --output schema.graphql",
    "svg:transform": "npx @svgr/cli --out-dir ./public/svg/components  --ignore-existing --typescript -- ./public/svg/origin",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@graphql-codegen/cli": "^2.12.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@react-google-maps/api": "^2.12.1",
    "@react-oauth/google": "^0.11.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/react-input-mask": "^2.0.4",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "apollo3-cache-persist": "^0.14.0",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog-for-jira-smart-commits": "^1.0.5",
    "dayjs": "^1.11.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "filesize": "^9.0.11",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.4.1",
    "i18next-localstorage-backend": "^3.1.3",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^3.0.1",
    "libphonenumber-js": "^1.10.14",
    "lint-staged": "^13.0.0",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "next-i18next": "^11.0.0",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.1",
    "react-hook-form": "^7.32.1",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-input-mask": "^2.0.4",
    "react-input-mask-next": "^3.0.0-alpha.12",
    "react-intersection-observer": "^9.3.5",
    "react-loader-spinner": "^5.1.5",
    "react-phone-input-2": "^2.15.0",
    "react-select": "^5.3.2",
    "react-switch": "^7.0.0",
    "react-textarea-autosize": "^8.3.4",
    "react-toastify": "^9.0.6",
    "styled-components": "^5.3.5",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "^8.3.0",
    "uuid": "^8.3.2",
    "validate-branch-name": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.9",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.182",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "react-helmet-async": "^1.3.0",
    "typescript": "4.7.3"
  },
  "validate-branch-name": {
    "pattern": "^(master|staging|develop){1}$|^ARENT-[0-9]{1,5}-(feature|fix|chore|hotfix|bugfix)/.+$|^(feature|fix|chore|hotfix|bugfix)/.+$",
    "errorMsg": "Invalid branch name"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-for-jira-smart-commits"
    }
  }
}
